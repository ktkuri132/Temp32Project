      "BSP/stm32f4/Driver/misc.c",
      "BSP/stm32f4/Driver/nvic.c",
      "BSP/stm32f4/Driver/Systick.c",
      "BSP/stm32f4/Driver/usart.c",
      "Control/filter.c",
      "Control/pid.c",
      "Device/bmp280/bmp280.c",
      "Device/hmc588/hmc588.c",
      "Device/mpu6050/inv_mpu.c",
      "Device/mpu6050/inv_mpu_dmp_motion_driver.c",
      "Device/sh1106/fonts.c",
      "Device/sh1106/sh1106.c",
