{
  "project": {
    "name": "General_template_Project",
    "chip": "STM32F407VE",
    "board": "BLUEPILL",
    "architecture": "cortex-m4",
    "instruction_set": "thumb",
    "float_type": "硬件浮点 (FPU: fpv4-sp-d16)"
  },
  "toolchain": {
    "c_compiler": "arm-none-eabi-gcc",
    "cxx_compiler": "arm-none-eabi-g++",
    "asm_compiler": "arm-none-eabi-gcc",
    "ar": "arm-none-eabi-ar",
    "objcopy": "arm-none-eabi-objcopy",
    "size": "arm-none-eabi-size"
  },
  "build": {
    "optimization": "O0",
    "debug_info": "g3",
    "warnings": false,
    "wall": false,
    "werror": false,
    "stack_usage": true
  },
  "linker": {
    "specs": [
      "nosys.specs",
      "nano.specs"
    ],
    "script": "BSP/stm32f4/CORE/stm32f4xx.ld",
    "gc_sections": true,
    "map_file": true,
    "printf_float": true,
    "scanf_float": true
  },
  "defines": [
    "STM32F40_41xxx",
    "STM32F10X_MD"
  ],
  "download": {
    "interface": "cmsis-dap",
    "target": "stm32f4x",
    "speed": "50000"
  },
  "files": {
    "sources": [
      "app/control.c",
      "app/init.c",
      "app/main.c",
      "BSP/stm32f4/CORE/startup_stm32f40_41xxx.c",
      "BSP/stm32f4/CORE/stm32f4xx_it.c",
      "BSP/stm32f4/CORE/system_stm32f4xx.c",
      "BSP/stm32f4/Driver/adc.c",
