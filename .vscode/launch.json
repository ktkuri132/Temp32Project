{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32 (J-Link)",
      "cwd": "${workspaceFolder}",
      "executable": "./build/Temp32Project.elf",
      "request": "launch",
      "type": "cortex-debug",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "raw",
      "servertype": "jlink",
      "device": "STM32F103C8",
      "interface": "swd",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 72000000,
        "swoFrequency": 2000000,
        "source": "probe",
        "decoders": [
          {
            "type": "console",
            "label": "ITM",
            "port": 0
          },
          {
            "type": "advanced",
            "decoder": "dwt",
            "label": "DWT",
            "ports": [
              {
                "number": 1,
                "type": "variable",
                "name": "Variable Watch",
                "enabled": true
              }
            ]
          }
        ]
      },
      "rttConfig": {
        "enabled": true,
        "address": "auto",
        "decoders": [
          {
            "port": 0,
            "type": "console",
            "label": "RTT Terminal"
          }
        ]
      },
      "graphConfig": [
        {
          "label": "Real-time Variables",
          "timespan": 10,
          "maximum": 100,
          "minimum": -100,
          "variables": [
            {
              "name": "variable_name",
              "address": "0x20000000"
            }
          ]
        }
      ],
      "preLaunchCommands": [
        "set mem inaccessible-by-default off",
        "enable breakpoint",
        "monitor SWO EnableTarget 72000000 2000000 1 0"
      ],
      "postLaunchCommands": [
        "monitor reset",
        "monitor halt"
      ],
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      },
      "armToolchainPath": "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.10/bin",
      "gdbPath": "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.10/bin/arm-none-eabi-gdb.exe",
      "jlinkGDBServerPath": "C:/Program Files/SEGGER/JLink_V862/JLinkGDBServerCL.exe"
    }
  ]
}