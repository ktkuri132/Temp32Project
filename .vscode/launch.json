{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug STM32",
      "cwd": "${workspaceFolder}",
      "executable": "./build/Temp32Project.elf",
      "request": "launch",
      "type": "cortex-debug",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "raw",
      "servertype": "openocd",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/stm32f1x.cfg"
      ],
      "searchDir": [
        "${workspaceFolder}",
        "C:/openocd/share/openocd/scripts/"
      ],
      "openOCDLaunchCommands": [
        "adapter speed 4000"
      ]
    },
    {
      "name": "Debug STM32 (CMSIS-DAP)",
      "cwd": "${workspaceFolder}",
      "executable": "./build/Temp32Project.elf",
      "request": "launch",
      "type": "cortex-debug",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "raw",
      "servertype": "openocd",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/stm32f1x.cfg"
      ],
      "searchDir": [],
      "openOCDLaunchCommands": [
        "adapter speed 4000"
      ],
      "svdFile": "",
      "swoConfig": {
        "enabled": true,
        "cpuFrequency": 72000000,
        "swoFrequency": 2000000,
        "source": "probe",
        "decoders": [
          {
            "type": "console",
            "label": "ITM",
            "port": 0
          },
          {
            "type": "advanced",
            "decoder": "dwt",
            "label": "DWT",
            "ports": [
              {
                "number": 1,
                "type": "variable",
                "name": "Variable Watch",
                "enabled": true
              }
            ]
          }
        ]
      },
      "liveWatch": {
        "enabled": true,
        "samplesPerSecond": 4
      },
      "armToolchainPath": "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.10/bin",
      "gdbPath": "C:/Program Files (x86)/GNU Arm Embedded Toolchain/10 2021.10/bin/arm-none-eabi-gdb.exe"
    }
  ]
}